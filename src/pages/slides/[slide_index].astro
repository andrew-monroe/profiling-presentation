---
export const PAGES = [
  "/intro",
  "/what-is-profiling",
  "/what-is-profiling-2",
  "/how-does-profiling-work",
  "/tools",
  "/diy",
  "/diy-2",
];

export function getStaticPaths() {
  return PAGES.map((_, idx) => ({ params: { slide_index: idx + 1 } }));
}

return Astro.rewrite(PAGES[Number(Astro.params.slide_index) - 1]);
---
